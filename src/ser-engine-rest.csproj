<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <RootNamespace>Ser.Engine.Rest</RootNamespace>
    <Product>Ser.Engine.Rest</Product>
    <PackageId>Ser.Engine.Rest</PackageId>
    <Authors>Konrad Mattheis und Martin Berthold</Authors>
    <Company>senseexcel</Company>
    <ApplicationIcon />
    <OutputType>Exe</OutputType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>SerEngineRest</AssemblyName>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>SerEngineRest.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Aspose.Cells" Version="19.2.0" />
    <PackageReference Include="Aspose.PDF" Version="19.2.0" />
    <PackageReference Include="enigma.net" Version="0.14.0-alpha0015" />
    <PackageReference Include="FluentCommandLineParser-netstandard" Version="1.4.3.13" />
    <PackageReference Include="Hjson" Version="3.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.App" Version="2.1.8" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="NLog" Version="4.5.11" />
    <PackageReference Include="NLog.Extensions.Logging" Version="1.4.0" />
    <PackageReference Include="NLog.Web.AspNetCore" Version="4.8.0" />
    <PackageReference Include="Q2gHelperQrs" Version="1.3.0" />
    <PackageReference Include="qlik-engineapi" Version="0.3.0-alpha0015" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="5.0.0-beta" />
    <PackageReference Include="Swashbuckle.AspNetCore.Annotations" Version="5.0.0-beta" />
    <PackageReference Include="System.Drawing.Common" Version="4.5.1" />
  </ItemGroup>

  <ItemGroup>
    <Content Update="App.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Update="Dockerfile">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Q2gHelperQlik">
      <HintPath>Lib\Q2gHelperQlik.dll</HintPath>
    </Reference>
    <Reference Include="SE-L">
      <HintPath>Lib\SE-L.dll</HintPath>
    </Reference>
    <Reference Include="ser-engine">
      <HintPath>..\..\ser-engine\src\bin\Debug\netcoreapp2.1\ser-engine.dll</HintPath>
    </Reference>
    <Reference Include="SerApi">
      <HintPath>Lib\SerApi.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
